{
  "$schema": "https://raw.githubusercontent.com/WordPress/gutenberg/trunk/schemas/json/block.json",
  "apiVersion": 2,
  "name": "qms4/area-list",
  "title": "エリア一覧",
  "category": "widgets",
  "icon": "pets",
  "attributes": {
    "href": {
      "type": "string",
      "default": ""
    },
    "layout": {
      "type": "string",
      "enum": [
        "hierarchical",
        "flat"
      ],
      "default": "hierarchical"
    },
    "targetBlank": {
      "type": "boolean",
      "default": false
    },
    "hideEmpty": {
      "type": "boolean",
      "default": true
    },
    "postTypes": {
      "type": "array",
      "default": []
    }
  },
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css"
}