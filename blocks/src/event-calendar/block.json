{
	"$schema": "https://raw.githubusercontent.com/WordPress/gutenberg/trunk/schemas/json/block.json",
	"apiVersion": 2,
	"name": "qms4/event-calendar",
	"version": "0.1.0",
	"title": "イベントカレンダー",
	"category": "widgets",
	"icon": "pets",
	"attributes": {
		"postType": {
			"type": "string",
			"default": ""
		},
		"showPosts": {
			"type": "boolean",
			"default": false
		},
		"showArea": {
			"type": "boolean",
			"default": false
		},
		"showTerms": {
			"type": "boolean",
			"default": true
		},
		"taxonomies": {
			"type": "array",
			"default": []
		},
		"linkFormat": {
			"type": "string",
			"default": ""
		},
		"linkTarget": {
			"type": "string",
			"default": "_self"
		}
	},
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css"
}
